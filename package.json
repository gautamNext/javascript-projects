{
  "name": "vanilla-javascript-projects",
  "version": "1.0.0",
  "description": "This repo contains some of good problems solved using vanilla JS only",
  "main": "index.js",
  "scripts": {
    "clean": "rm dist/bundle.js",
    "dev": "webpack-dev-server --open",
    "build": "node node_modules/.bin/webpack --mode production",
    "format": "run-s --continue-on-error format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier --write .",
    "lint": "run-s --continue-on-error lint:*",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --format codeframe",
    "lint:prettier": "prettier --list-different .",
    "pretest": "yarn format && yarn build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@gautamrk:gautamNext/vanilla-javascript-projects.git"
  },
  "author": "gautamNext",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^6.7.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-import": "^2.25.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "pretty-quick": "^3.1.3",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
